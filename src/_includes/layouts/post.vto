{{ layout  "layouts/base.vto" }}

<div class="article-wrapper">
    <article>

        <header>
            <h1>{{ title }}</h1>
        </header>

        <nav class="toc">
            {{ content |> toc }}
        </nav>

        <section class="content">
            {{ content }}
        </section>

    </article>
</div>
{{ /layout }}
